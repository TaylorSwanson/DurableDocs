{
	"compilerOptions": {
		"target": "esnext",
		"lib": [
			"esnext"
		],
		"strict": true,
		"alwaysStrict": true,
		"noEmitOnError": true,
		"moduleResolution": "node",
		"outDir": "out",
		"removeComments": false,
		"types": [
			"@cloudflare/workers-types"
		],
		"noImplicitAny": false,
		"declaration": true
	},
	"include": [
		"src",
		"test"
	],
	"exclude": [
		"node_modules"
	]
}
